<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- 색상 -->
    <Color x:Key="BackgroundBlue">#3099c5</Color>
    <SolidColorBrush x:Key="BackgroundBlueBrush" Color="{StaticResource BackgroundBlue}"/>
    
    <!-- 오렌지 -->
    <Color x:Key="OnjOrange">#ffa800</Color>
    <SolidColorBrush x:Key="OnjOrangeBrush" Color="{StaticResource OnjOrange}"/>
</ResourceDictionary>